{
    "lambda": {
        "deploy_settings": {
            "default": {
                "ask_profile": "default",
                "skill_id": "{skillId}",
                "was_cloned": false,
                "merge": {
                    "manifest": {
                        "apis": {
                            "custom": {
                                "endpoint": {
                                    "uri": "{lambdaArn}"
                                }
                            }
                        }
                    }
                }
            }
        }
    },
    "local": {
        "deploy_settings": {
            "default": {
                "ask_profile": "default",
                "skill_id": "{skillId}",
                "was_cloned": false
            }
        }
    }
}
